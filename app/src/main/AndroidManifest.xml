<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.liaction.ymwy.myapplication">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <!--<uses-permission android:name="com.liaction.ymwy.aidl.service"/>-->

    <application
            android:name="com.liaction.ymwy.myapplication.base.YMWYBaseApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <service
                android:permission="com.liaction.ymwy.aidl.service"
                android:process=":aidl_service"
                android:name=".service.YMWYAidlService"
                android:enabled="true"
                android:exported="true">
        </service>

        <activity android:name=".act.YMWYIPCAidlActivity">
        </activity>
        <activity android:name=".act.YMWYLayoutActivity">
        </activity>
        <activity android:name=".act.ViewEventActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".act.YMWYCreateActivity"/>
    </application>

</manifest>